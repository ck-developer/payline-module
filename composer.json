{
    "name": "ck-developer/payline-module",
    "description": "Payline module for Zend Framework 2",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "claude khedhiri",
            "email": "claude@khedhiri.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-soap": "*",
        "ext-curl": "*",
        "zendframework/zendframework": "2.*",
        "monext/payline-sdk": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "PaylineModule": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "PaylineModuleTest": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
